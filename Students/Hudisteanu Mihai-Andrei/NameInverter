//split
//trim spatii de la final si inceput
public class Main {

	public static String invertor(String t){
		String outString;
		if(t == null){
			return null;
		}
		
		String y = "";
	    String z = "";
	    String[] w = t.split("\\s+");//pune in w toate cuvintele din t pe linii separate
	    //for(String word :w){
	    //	System.out.println(word);
	    //}
	   
	    for(String word :w){			
	    	if(word.contains(".")){
	    		y= y.concat(" ");
	    		y=y.concat(word);//concateneaza in y cuvintele care au "."
	    	}
	    	else {
	    		z =word + " " + z; //daca nu contin punct le pune in z
	    	}
	    }
	    z=z.trim();				//taie spatii de la inceput si de la sfarsit (in cazul in care au mai ramas)
	    //y=y.trim();
	    outString = z + y; 	    //concateneaza cele doua siruri in outString 
	    return outString; 
	}
	
	public static void main (String args[])
	{
		
		System.out.println("||  =" + invertor(""));
		System.out.println("|  hello  |  =" + invertor("  hello  "));
		System.out.println("|  hello  world  |  =" + invertor("  hello  world "));
		System.out.println("|SingleName|  =" + invertor("SingleName"));
		System.out.println("| SingleName |  =" + invertor(" SingleName "));
		System.out.println("|FirstName LastName|  =" + invertor("FirstName LastName"));
		System.out.println("|  FirstName LastName  |  =" + invertor("FirstName LastName"));
		System.out.println("| Mr.  FirstName LastName  |  =" + invertor("Mr. FirstName LastName"));
		System.out.println("| Mr. Phd.  FirstName LastName  |  =" + invertor(" Mr. Phd. FirstName LastName"));
		
	
	}
	
}
